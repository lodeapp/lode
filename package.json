{
  "private": true,
  "name": "Lode",
  "version": "0.29.0",
  "author": "Tomas Buteler <info@lode.run>",
  "description": "A universal GUI for unit testing",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "homepage": "https://lode.run",
  "repository": "github:lodeapp/lode",
  "issues": {
    "url": "https://github.com/lodeapp/lode/issues",
    "email": "info@lode.run"
  },
  "scripts": {
    "icons": "node workflow/icons.js",
    "licenses": "node workflow/licenses.js",
    "decipher": "node workflow/decipher.js",
    "migrate:up": "cross-env LOGGER=false LODE_MIGRATE=false node workflow/dev-runner.js migrate up",
    "migrate:down": "cross-env LOGGER=false LODE_MIGRATE=false node workflow/dev-runner.js migrate down",
    "pack:lode": "node workflow/build.js",
    "pack:jest": "cross-env NODE_ENV=production webpack --mode production --progress --colors --config workflow/reporters/webpack.jest.config.js",
    "pack:phpunit": "node workflow/reporters/phpunit.js",
    "pack:reporters": "yarn run pack:jest && yarn run pack:phpunit",
    "dev": "yarn run icons && cross-env IS_DEV=true node workflow/dev-runner.js",
    "clean": "node workflow/clean.js",
    "clean:all": "yarn run clean:build && yarn run clean",
    "clean:build": "node workflow/clean-build.js",
    "build:before": "yarn run clean:all && yarn run icons && yarn run pack:reporters && yarn run licenses",
    "build:after": "yarn run clean",
    "build": "yarn run build:before && yarn run pack:lode && cross-env NOTARIZE=false electron-builder --publish never && yarn run build:after",
    "build:dev": "cross-env IS_DEV=true yarn run build",
    "release:before": "node workflow/release-before.js && yarn run build:before",
    "release:after": "node workflow/release-after.js && yarn run build:after",
    "release": "yarn run release:before && yarn run pack:lode && electron-builder --publish always && yarn run release:after",
    "coverage": "yarn run tests --coverage",
    "test": "jest --config .jestrc.json",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "size": "SIZE=true yarn run dev",
    "size:main": "SIZE=true PROCESS=main yarn run dev"
  },
  "dependencies": {
    "@bugsnag/js": "^6.4.0",
    "@primer/css": "^14.3.0",
    "@sentry/electron": "^1.3.0",
    "bottleneck": "^2.13.0",
    "custom-electron-titlebar": "^3.1.0",
    "decimal.js": "^10.2.0",
    "electron-store": "^4.0.0",
    "electron-updater": "^4.1.2",
    "electron-window-state": "^5.0.2",
    "fs-extra": "^7.0.0",
    "fuzzysearch": "^1.0.3",
    "glob": "^7.1.3",
    "has-ansi": "^3.0.0",
    "highlight.js": "^9.15.6",
    "is-unc-path": "^1.0.0",
    "latinize": "^0.4.1",
    "lodash": "^4.17.13",
    "markdown-it": "^10.0.0",
    "overlayscrollbars": "^1.7.1",
    "sha1-es": "^1.8.2",
    "shell-env": "^2.2.0",
    "shell-escape": "^0.2.0",
    "split.js": "^1.5.10",
    "strip-ansi": "^5.0.0",
    "terminal.js": "^1.0.10",
    "tree-kill": "^1.2.1",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2",
    "vue": "^2.5.16",
    "vuex": "^3.0.1",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/is-unc-path": "^1.0.0",
    "@types/latinize": "^0.2.15",
    "@types/lodash": "^4.14.116",
    "@types/markdown-it": "^0.0.8",
    "@types/node": "^10.9.4",
    "@types/shell-escape": "^0.2.0",
    "@types/uuid": "^3.4.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "aws-sdk": "^2.528.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "^5.0.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "dotprejs": "^0.0.2",
    "electron": "^6.0.2",
    "electron-builder": "^22.5.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.3.0",
    "eslint": "^6.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.3.1",
    "jest": "^25.4.0",
    "license-checker": "^25.0.1",
    "mini-css-extract-plugin": "0.4.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "parse-author": "^2.0.0",
    "require-dir": "^1.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.0.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
